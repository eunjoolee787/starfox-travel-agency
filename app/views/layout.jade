  title Start Intergalacitic Travel Agency 
  meta(name="viewport" content="width=device-width, initial-scale=1.0")
  script(src="/bower_components/modernizr/modernizr.js")
  link(rel="stylesheet" href="/css/styles.css")

body
  header#pageHeader
    div.logoImg
    div.bars
